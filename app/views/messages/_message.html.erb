<div class="chat-message">
  <div class="upper-message">
    <div class="message-user">
      <%= message.user.name%>
    </div>
    <div class="date">
      <%= message.created_at%>
    </div>
  </div>
  <div class="lower-message">
    <div class="message">
      <%= message.content%>
    </div>
  </div>
</div>